
using io : "eokas.io";

func main(args: array<string>): i32 {
    // 'self' is this function.

    var fib = func(x: i32):i32 {
        if(x == 1 || x == 2)
            return 1;
        return self(x-1) + self(x-2);
    };

    var i = 10;
    loop(var i = 1; i <= 10; i=i+1) {
        io.print(fib(i));
    }
}
