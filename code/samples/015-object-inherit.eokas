
using io : "eokas.io";

func main(args: array<string>): i32 {
    struct Object {
        var name: string;
    }

    struct Asset : Object {
        var path: string;
    }

    var asset = make Asset{
        name = "eokas",
        path = "https://asset.example.com"
    };

    io.print(asset.name);
    io.print(asset.path);
}
