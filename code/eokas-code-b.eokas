schema S {
    x: float;
};

schema X {
    x: string;
};

schema A {
    a: int;
    b: bool;
    o: X;
};

struct B : A {
    a: int = 0;
    b: bool = true;
    o: X = null;
};

schema A<T: S> {
    x: T;
};

struct B<T> : A<T> {
    x: T;
};

func F(a: int, b: int): int;

val a : A = null;

val f : F = func(a: int, b: int) : int {
    return a + b;
};

val f = func(a: int, b: int): int {
    return a + b;
};
