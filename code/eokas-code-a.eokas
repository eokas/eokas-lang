type S = schema {
    x: float;
};

type A = schema {
    a: int;
    b: bool;
    o: schema {
        name: string;
    };
};

type B = struct : A {
    a: int = 0;
    b: bool = true;
    o: schema {
        x: name;
    } = null;
};

type A = schema<T: S> {
    x: T;
};

type B = struct<T> : A<T> {
    x: T;
};

type F = func(a: int, b: int):int;

val a : A = null;

val f : F = func(a:int, b:int):int{
    return a + b;
};

val f = func(a: int, b:int): int {
    return a + b;
};
